{

    "name": "Blueprint",
    "version": "1.0",
    "minimum_chrome_version": "10.0",
    "description": "Visualize data flow for smoother application development",
    "permissions": ["storage"],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "devtools_page": "index.html",
    "manifest_version": 2,
    "content_security_policy": "script-src 'self'; object-src 'self'",
    "web_accessible_resources": [
        "node_modules/*",
        "build/*"
    ]

}