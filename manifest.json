{
  "name": "Blueprint",
  "version": "0.1.0",
  "description": "A Performance Visualization Tool for Optimization and Debugging",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "exclude_globs": [
        "https://www.google*"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "content_security_policy": "",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "devpanel.bundle.js"
  ],
  "manifest_version": 2,
  "devtools_page": "devtools.html",
  "permissions": [
    "<all_urls>",
    "tabs"
  ],
  "externally_connectable": {
    "ids": [
      "*"
    ]
  }
}